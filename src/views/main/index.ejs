<!DOCTYPE html>
<html lang="es">
<head>
    <%- include('../partials/head') %>

    <title>Index EliBaExpress</title>
</head>
<body>
    <%- include('../partials/header') %>

    <div class=" m-2 mt-3 alert alert-primary <%= locals.message && message != '' ? '' : 'd-none'  %> " role="alert"> <%= locals.message && message  %> </div>

    <h1 class="text-center">Index</h1>

    <form method="post" id="formUploadImageKit">
        <input type="file" name="image" id="inputImage">
        <input type="email" name="email" value="email@gmail.com" id="emaill">
        <button type="submit">Enviar</button>
    </form>

    <form action="/imagekit/delete" method="post">
        <input type="text" name="imageId" id="">
        <input type="submit" value="Eliminar Imagen">
    </form>

    <script src="/js/testImageKit.js"></script>
    <%- include('../partials/footer') %>

</body>
</html>